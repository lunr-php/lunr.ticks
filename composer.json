{
  "name": "lunr/ticks",
  "description": "Lunr.Ticks is a library for standardized profiling and event logging.",
  "keywords": [ "profiler", "profiling", "event logging" ],
  "homepage": "https://github.com/lunr-php/lunr.ticks",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "lunr/halo": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.0 <9.6",
    "ext-xdebug": "~3.1"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {"Lunr\\": "src/Lunr/"}
  }
}
